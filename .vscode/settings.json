    "editor.minimap.enabled": false,
    "editor.tokenColorCustomizations":{
        "[Community Material Theme Ocean]": {
            "textMateRules": [
                {
                    "scope": [
                        "comment.block",
                        "comment.block.documentation",
                        "comment.line",
                        "constant",
                        "constant.character",
                        "constant.character.escape",
                        "constant.numeric",
